  "dependencies": {
    "@emotion/core": "10.0.10",
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "escape-string-regexp": "2.0.0",
    "gatsby": "2.8.3",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-netlify-cms": "4.0.1",
    "gatsby-plugin-offline": "2.1.0",
    "gatsby-source-filesystem": "2.0.34",
    "gatsby-transformer-remark": "2.3.12",
    "moment": "2.24.0",
    "netlify-cms-app": "2.9.1",
    "npm-font-open-sans": "1.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "babel-jest": "24.8.0",
    "babel-preset-gatsby": "0.1.11",
    "cypress": "3.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.4.0",
    "mocha": "5.2.0",
    "mochawesome": "4.0.0",
    "prettier": "1.17.1",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.0.1",
    "start-server-and-test": "1.9.1"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "npm audit ; jest ; gatsby build",
    "cy:open": "node_modules/cypress/bin/cypress open",
    "cy:run": "node_modules/cypress/bin/cypress run",
    "cy:run:chrome": "node_modules/cypress/bin/cypress run -b chrome",
    "cy:run:chrome:specific": "node_modules/cypress/bin/cypress run -b chrome -s",
    "develop": "gatsby develop",
    "format": "prettier --write {cypress,src}/**/*.{js,jsx}",
    "start": "npm run develop",
    "local": "./localbuild.sh; npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "e2e": "start-server-and-test start http://localhost:8000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/census-worth-self-help.git"
  },
  "bugs": {

{
  "name": "census-worth-self-help",
  "private": true,
  "description": "A Self Help Facility",
  "version": "0.1.0",
  "license": "MIT",
  "main": "app.js",
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-regular-svg-icons": "5.8.2",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.3.0",
    "@reach/visually-hidden": "0.1.4",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cookie-session": "1.3.3",
    "crypto-js": "^4.0.0",
    "dotenv": "8.0.0",
    "escape-string-regexp": "2.0.0",
    "express-static-zip": "file:vendor/express-static-zip",
    "gatsby": "^2.30.2",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-manifest": "^2.2.7",
    "gatsby-plugin-matomo": "0.7.2",
    "gatsby-plugin-netlify-cms": "4.1.2",
    "gatsby-plugin-offline": "2.1.0",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-source-filesystem": "2.0.34",
    "gatsby-transformer-remark": "2.3.12",
    "hsts": "2.2.0",
    "immutable": "^4.0.0-rc.12",
    "localforage": "^1.7.3",
    "moment": "2.24.0",
    "netlify-cms-app": "^2.12.11",
    "netlify-cms-media-library-uploadcare": "^0.5.5",
    "netlify-cms-ui-default": "2.11.2",
    "node-fetch": "^2.6.0",
    "npm-font-open-sans": "1.1.0",
    "passport": "0.4.0",
    "passport-saml": "1.1.0",
    "react": "16.8.6",
    "react-aria-menubutton": "6.2.0",
    "react-children-utilities": "1.2.1",
    "react-datepicker": "^2.10.0",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-select": "3.0.4",
    "sanitize-html-react": "1.13.0",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "babel-jest": "24.8.0",
    "babel-preset-gatsby": "^0.1.11",
    "chai": "4.2.0",
    "cypress": "^3.4.1",
    "cypress-plugin-retries": "1.2.2",
    "husky": "^4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.1.0",
    "jest-dom": "3.4.0",
    "mocha": "5.2.0",
    "mochawesome": "4.0.0",
    "mochawesome-merge": "2.0.1",
    "mochawesome-report-generator": "4.0.0",
    "mockdate": "2.0.3",
    "prettier": "1.17.1",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.0.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "1.9.1"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "npm audit && npm run build-no-audit",
    "build-no-audit": "jest && npm run build-no-audit-no-test",
    "build-no-audit-no-test": "gatsby build && ./archive-content-files.sh",
    "clean": "rimraf public/ node_modules/.cache/ .cache/",
    "coverage": "jest --coverage",
    "cy-open": "node_modules/cypress/bin/cypress open",
    "cy-run": "node_modules/cypress/bin/cypress run --spec cypress/integration/ui_tests/e2e_tests/**/*",
    "cy-secured-test": ". ./cypress_env_variables.sh; ./node_modules/.bin/cypress run --spec cypress/integration/ui_tests/secured_request_test/**/*",
    "cy-secured-open": ". ./cypress_env_variables.sh; node_modules/cypress/bin/cypress open",
    "develop": "gatsby develop",
    "e2e": "export EXCLUDE_DRAFTS='true'; start-server-and-test develop http://localhost:8000 cy-run",
    "e2e-test-runner": "export EXCLUDE_DRAFTS='true'; start-server-and-test develop http://localhost:8000 cy-open",
    "e2e-secured-test": "export EXCLUDE_DRAFTS='true'; start-server-and-test develop http://localhost:8000; . ./cypress_env_variables.sh; ./node_modules/.bin/cypress run --spec cypress/integration/ui_tests/secured_request_test/**/*",
    "e2e-secured-open": "export EXCLUDE_DRAFTS='true'; start-server-and-test develop http://localhost:8000; . ./cypress_env_variables.sh; node_modules/cypress/bin/cypress open",
    "format": "prettier --write {app,cypress,src}/**/*.{js,jsx}",
    "local": "./localbuild.sh; npm run develop",
    "local:production-like": "./localbuild.sh; export EXCLUDE_DRAFTS='true' ; npm run develop",
    "merge-reports": "npx mochawesome-merge --reportDir mochawesome-report > mochawesome.json; npx mochawesome-report-generator mochawesome.json --reportDir mochawesome-report",
    "serve": "gatsby serve",
    "start": "node app.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/census-worth-self-help.git"
  },
  "bugs": {
    "url": "https://worthit.jira.com/browse/ONS"
  }
}
